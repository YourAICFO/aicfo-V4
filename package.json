{
  "name": "aicfo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run db:up && npx concurrently -n api,web -c blue,green \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --prefix backend",
    "dev:frontend": "npm run dev --prefix frontend",
    "db:up": "docker compose up -d postgres redis",
    "db:down": "docker compose down",
    "db:migrate": "npm run db:migrate --prefix backend",
    "db:migrate:status": "npm run db:migrate:status --prefix backend",
    "db:seed": "cd backend && node migrations/seed.js",
    "test": "npm test --prefix backend",
    "typecheck": "npm run typecheck --prefix frontend",
    "build": "npm run build --prefix frontend",
    "install:all": "npm install --prefix backend && npm install --prefix frontend"
  },
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
